	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "NODE_ENV=development vite dev",
        "build": "NODE_ENV=production vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
